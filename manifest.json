{
  "name": "Paero",
  "version": "1.0",
  "manifest_version": 2,
  "description": "A minimalistic new tab page.",
  "options_page": "options/options.html",

  "permissions": [
    "history",
    "tabs",
    "storage",
    "contextMenus",
    "<all_urls>"
  ],

  "chrome_url_overrides": {
    "newtab": "tab_page/paero.html"
  }
}